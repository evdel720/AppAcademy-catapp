<form action="/cat_rental_requests" method="post">
  <label>
    Cat :
    <select name="cat_rental_request[cat_id]">
      <option value="Select Cat" disabled selected>Select Cat</option>
      <% cats.each do |cat| %>
      <option value="<%=cat.id%>"><%=cat.name%></option>
      <% end %>
    </select>
  </label>

  <label>
    Start Date :
    <input type="date" name="cat_rental_request[start_date]">
  </label>

  <label>
    End Date :
    <input type="date" name="cat_rental_request[end_date]">
  </label>

  <input type="submit" value="Submit Request">
</form>
